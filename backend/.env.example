# CBB Betting App Environment Variables
# Copy this file to .env and fill in your actual values

# Required API Keys
KENPOM_API_KEY=your_kenpom_api_key_here
ODDS_API_KEY=your_odds_api_key_here

# Database (optional - defaults to SQLite for development)
# DATABASE_URL=postgresql://user:password@localhost:5432/cbb_betting
# DATABASE_URL=sqlite:///./cbb_betting.db

# Debug mode (optional)
DEBUG=false

# CORS allowed origins (optional - has sensible defaults)
# ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
